{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],

  "jsRules": {},

  "rules": {
    "prettier": true,
    "interface-name": [true, "never-prefix"],
    "no-empty-interface": false,
    "max-classes-per-file": [true, 5],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-leading-underscore"
    ]
  }
}
